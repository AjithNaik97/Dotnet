@{
    List<Students> students = (List<Students>)ViewData["catalog"];
    <h1>Students Sorted By Name</h1>
    <ul>
    @{
        foreach(Students stud in students)
        {
                <li>
                    @stud.Id
                    @stud.Name
                    @stud.Email
                    @stud.Phone
                    @stud.Address
                    @stud.Admission_Date
                    @stud.Fees
                    @stud.Status
                </li>
        }
    }
    </ul>
}
