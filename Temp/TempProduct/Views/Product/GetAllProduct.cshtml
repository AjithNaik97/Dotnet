@{
    List<Products> products = (List<Products>)ViewData["catalog"];
    <h1>LIST OF PRODUCTS</h1>
    <table border="2" >
     <tr>
        <td>ID</td>
        <td>Title</td>
        <td>Description</td>
        <td>UnitPrice</td>
        <td>Quantity</td>
        <td>ImageUrl</td>
     </tr>
    @{
    foreach (Products product in products)
        {
                    <tr>
                    <td>@product.Id</td>
                    <td>@product.Title </td>
                    <td>@product.Description </td>
                    <td> @product.UnitPrice</td>
                    <td>@product.Quantity  </td>
                    <td>@product.ImageUrl </td>
                    </tr>
        }
    }
    </table>
}
